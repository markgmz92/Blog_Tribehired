<h1>List of all Posts</h1>
<table>
<tr>
<th>Title</th>
<th>Body</th>
<tr>
<% @posts.each do |post| %>
</tr>
    <td>
        <%= post.title %>
    </td>
    <td>
        <%= truncate(post.text, length: 75) %>
    </td>
    <td>
    <%= link_to "Show", post_path(post) %>
    </td>
</td>
</tr>
<% end %>
<table>
<p>
    <%= link_to 'Home', welcome_index_path %>
</p>